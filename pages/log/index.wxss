.page-log {
    background-color: #fdfdfd;
}

.tab-list {
    position: fixed;
    top: 130rpx;
    left: 0;
    right: 0;
    z-index: 10000;
    background-color: #fdfdfd;
    display: flex;
}
.iphone-x .tab-list {
    top: 180rpx;
}
.tab-list .item {
    display: flex;
    justify-content: center;
    align-items: center;
    width: 100%;
    height: 72rpx;
}
.tab-list .text {
    line-height: 72rpx;
    font-size: 30rpx;
    border-bottom: 4px solid #fdfdfd;
}
.tab-list .active .text {
    color: #4285f4;
    border-color: #4285f4;
}

.log-list {
    padding-top: 72rpx;
    padding-bottom: 220rpx;
}
.log-list .item {
    position: relative;
    height: 238rpx;
    padding: 30rpx 24rpx;
    background-color: #fff;
    border-bottom: 1px solid #eee;
}
.log-list .title {
    margin-bottom: 32rpx;
    font-size: 30rpx;
}
.log-list .state {
    position: absolute;
    top: 36rpx;
    right: 24rpx;
    color: #4285f4;
}
.log-list .state-pay {
    color: #f44242;
}
.log-list .time {
    display: flex;
    align-items: center;
    margin-bottom: 4rpx;
    color: #555;
    font-size: 26rpx;
}
.log-list .stop-time {
    position: absolute;
    bottom: 86rpx;
    right: 24rpx;
}
.log-list .money {
    position: absolute;
    bottom: 24rpx;
    right: 24rpx;
    color: #555;
}
.log-list .money-value {
    font-size: 30rpx;
    font-weight: bold;
}
.log-list .ui-icon {
    margin-right: 16rpx;
    color: #cacaca;
    font-size: 32rpx;
}